{
  "name": "esportube-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "now-build": "next build",
    "export": "npm run build && next export",
    "start": "NODE_ENV=production node server.js",
    "getschema": "gq https://esportube.herokuapp.com/v1/graphql -H 'X-Hasura-Admin-Secret: cb74937b1e6d11d58e0da6e84d0c304e' --format json --introspect > schema.json",
    "gcgen": "graphql-codegen --config codegen.yml",
    "apgen": "apollo codegen:generate --localSchemaFile=schema.json  --target=typescript --includes=queries/*.ts --tagName=gql --globalTypesFile=types/graphql-global-types.ts --addTypename types"
  },
  "dependencies": {
    "@types/md5": "^2.1.33",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.6",
    "@types/react-no-ssr": "^1.1.1",
    "@types/styled-components": "^4.1.11",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "constate": "^1.1.1",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "grommet": "^2.7.1",
    "grommet-icons": "^4.2.0",
    "grommet-styles": "^0.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "md5": "^2.2.1",
    "next": "8.1.0",
    "next-apollo": "^2.1.3",
    "next-progressbar": "^1.0.0",
    "query-string": "^6.8.1",
    "react": "16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "16.8.6",
    "react-no-ssr": "^1.1.0",
    "react-use": "^9.7.1",
    "react-use-session": "^1.1.0",
    "sanitize.css": "^10.0.0",
    "styled-components": "^4.1.3",
    "styled-spinkit": "^0.7.3",
    "toasted-notes": "^2.1.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.0",
    "@graphql-codegen/introspection": "1.3.0",
    "@graphql-codegen/typescript": "1.3.0",
